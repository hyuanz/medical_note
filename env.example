# Copy this file to .env and fill in your real values

# =======================
# AWS
# =======================
AWS_REGION=us-west-2
# Optional if you use named profiles with the AWS CLI
AWS_PROFILE=default

# Optional: explicit static credentials (prefer using AWS SSO or profiles)
# AWS_ACCESS_KEY_ID=
# AWS_SECRET_ACCESS_KEY=
# AWS_SESSION_TOKEN=

# DynamoDB table used by this project
DYNAMO_TABLE_NAME=clinical_notes

# =======================
# PostgreSQL
# =======================
POSTGRES_HOST=YOUR_POSTGRES_HOST
POSTGRES_PORT=5432
POSTGRES_DB=YOUR_DB_NAME
POSTGRES_USER=YOUR_DB_USER
POSTGRES_PASSWORD=YOUR_DB_PASSWORD

# Streaming/processing batch sizes (tune for performance)
POSTGRES_BATCH_SIZE=10000
DYNAMO_SCAN_BATCH_SIZE=10000

# =======================
# OpenAI
# =======================
# Required if you run LLM analysis in the notebooks
OPENAI_API_KEY=

# =======================
# App/Logging
# =======================
LOG_LEVEL=INFO
